                                android:layout_height="wrap_content"
                                android:text="Phone number"
                                android:textColor="@color/colorBlack"
                                android:textStyle="italic"
                                android:textSize="15sp"
                                android:id="@+id/phone_text"
                                android:layout_marginTop="2dp"
                        />
                        <View
